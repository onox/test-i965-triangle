<PropertyList>

    <path>windows.ac</path>

    <effect>
        <inherits-from>../Effects/exterior-glass</inherits-from>
        <object-name>exterior-glass</object-name>
        <object-name>glass-left-door</object-name>
        <object-name>glass-right-door</object-name>
    </effect>

    <effect>
        <inherits-from>../Effects/interior-glass-left</inherits-from>
        <object-name>interior-glass-left</object-name>
        <object-name>interior-glass-left-front</object-name>
        <object-name>interior-glass-left-door</object-name>
    </effect>

    <effect>
        <inherits-from>../Effects/interior-glass-right</inherits-from>
        <object-name>interior-glass-right</object-name>
        <object-name>interior-glass-right-front</object-name>
        <object-name>interior-glass-right-door</object-name>
    </effect>

    <!-- Go inside -->
    <animation>
        <type>rotate</type>
        <object-name>glass-right-door</object-name>
        <object-name>interior-glass-right-door</object-name>
        <property>sim/multiplay/generic/float[5]</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>-4.0</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>-4.0</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m> -18.812</x1-m>
            <y1-m>   0.751</y1-m>
            <z1-m>   0.955</z1-m>
            <x2-m> -18.805</x2-m>
            <y2-m>   0.745</y2-m>
            <z2-m>   0.973</z2-m>
        </axis>
    </animation>

    <!-- Slipping back -->
    <animation>
        <type>translate</type>
        <object-name>glass-right-door</object-name>
        <object-name>interior-glass-right-door</object-name>
        <property>sim/multiplay/generic/float[5]</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>-0.5</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m> -17.8</x1-m>
            <y1-m>   1.23</y1-m>
            <z1-m>   0.70</z1-m>
            <x2-m> -18.805</x2-m>
            <y2-m>   0.80</y2-m>
            <z2-m>   0.973</z2-m>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>glass-right-door</object-name>
        <object-name>interior-glass-right-door</object-name>
        <property>sim/multiplay/generic/float[5]</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>0.01</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>8.0</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m> -17.8</x1-m>
            <y1-m>   1.23</y1-m>
            <z1-m>   0.70</z1-m>
            <x2-m> -18.805</x2-m>
            <y2-m>   0.78</y2-m>
            <z2-m>   0.973</z2-m>
        </axis>
    </animation>

    <!-- Go inside -->
    <animation>
        <type>rotate</type>
        <object-name>glass-left-door</object-name>
        <object-name>interior-glass-left-door</object-name>
        <property>sim/multiplay/generic/float[4]</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>4.0</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>4.0</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m> -18.812</x1-m>
            <y1-m>  -0.751</y1-m>
            <z1-m>   0.955</z1-m>
            <x2-m> -18.805</x2-m>
            <y2-m>  -0.745</y2-m>
            <z2-m>   0.973</z2-m>
        </axis>
    </animation>

    <!-- Slipping back -->
    <animation>
        <type>translate</type>
        <object-name>glass-left-door</object-name>
        <object-name>interior-glass-left-door</object-name>
        <property>sim/multiplay/generic/float[4]</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>-0.5</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m> -17.8</x1-m>
            <y1-m>  -1.23</y1-m>
            <z1-m>   0.70</z1-m>
            <x2-m> -18.805</x2-m>
            <y2-m>  -0.80</y2-m>
            <z2-m>   0.973</z2-m>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>glass-left-door</object-name>
        <object-name>interior-glass-left-door</object-name>
        <property>sim/multiplay/generic/float[4]</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>0.01</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>-8.0</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m> -17.8</x1-m>
            <y1-m>  -1.23</y1-m>
            <z1-m>   0.70</z1-m>
            <x2-m> -18.805</x2-m>
            <y2-m>  -0.78</y2-m>
            <z2-m>   0.973</z2-m>
        </axis>
    </animation>

</PropertyList>
